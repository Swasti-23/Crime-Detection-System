                <input type="text" class="form-control mb-3" id="searchName" placeholder="Enter Name">
                <button onclick="animateButton()" id="Emer">
                    <img src="/Emergency.png" alt="Hey this is my description">
                </button>
            </div>
        </div>
    </div>
    
    

    <script>
        function animateButton() {
            var button = document.getElementById('Emer');
            button.style.transform = 'translateY(-5px)';
            setTimeout(function () {
                button.style.transform = 'translateY(5px)';
